{
  "name": "markdown-draft-js",
  "description": "Convert draftjs blocks to markdown using the marked library, and vice versa.",
  "author": "Rose Robertson",
  "version": "0.1.2",
  "main": "lib/index.js",
  "scripts": {
    "build": "browserify src -o lib/index.js",
    "prepublish": "npm run build",
    "test": "karma start --single-run",
    "lint": "eslint ./src",
    "lint:tests": "eslint ./test"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "7.3.0",
    "browserify": "13.1.0",
    "eslint": "3.8.0",
    "jasmine": "2.5.2",
    "karma": "1.3.0",
    "karma-browserify": "5.1.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.0.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "draft-js": "0.9.1",
    "markdown": "0.5.0",
    "react": "15.3.2",
    "react-dom": "15.3.2"
  }
}
